:VaeEntity rdf:type owl:Class ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasSetting> ;
                             owl:someValuesFrom :Context
                           ] ;
           rdfs:comment "Any kind of object from the VAE domain that is contextualized." ;
           rdfs:label "Vae Object" ;
           rdfs:seeAlso :Context .


###  https://w3id.org/def/vaeontology#VaeProperty
:VaeProperty rdf:type owl:Class ;
             rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description> ,
                             :VaeStatementEntity ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :measuredOnOutcome ;
                               owl:someValuesFrom :Outcome
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :propertyExpressedAt ;
                               owl:allValuesFrom :VaePropertyStatement
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :relationWith ;
                               owl:allValuesFrom :VaeConditioningEntity
                             ] ;
             rdfs:comment "Any property measured on an Outcome, useful to describe some aspect that is related to a Norm or a Value" ;
             rdfs:label "Vae Property" .


###  https://w3id.org/def/vaeontology#VaePropertyStatement
:VaePropertyStatement rdf:type owl:Class ;
                      rdfs:subClassOf :AgentStatement ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :expressesProperty ;
                                        owl:someValuesFrom :VaeProperty
                                      ] ;
                      rdfs:comment "An AgentStatement where the agent expresses some VaeProperty of an Outcome." ;
                      rdfs:label "Vae Property Statement" .


###  https://w3id.org/def/vaeontology#VaeSituation
:VaeSituation rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/odrl/2/Asset> ,
                              :VaeEntity ,
                              <https://w3id.org/framester/schema/FrameOccurrence> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies> ;
                                owl:someValuesFrom :VaeStatement
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies> ;
                                owl:someValuesFrom :ValueConcept
                              ] ;
              owl:disjointWith :VaeStatement ;
              rdfs:comment "Any kind of VaePropertyStatement proposed by an agent in which they express that a complex notion of understanding, preservation or respect with a ValueConcept/Norm is present on entity (Agent or Outcome or Norm) based on some VaeProperty(s) seen." ;
              rdfs:label "Vae Statement" .


###  https://w3id.org/def/vaeontology#VaeStatement
:VaeStatement rdf:type owl:Class ;
