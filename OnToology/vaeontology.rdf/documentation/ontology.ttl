###  http://www.w3.org/ns/odrl/2/Policy
<http://www.w3.org/ns/odrl/2/Policy> rdf:type owl:Class ;
                                     rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
                                     rdfs:label "Policy"@en ,
                                                "Policy" ;
                                     <http://www.w3.org/2004/02/skos/core#definition> "A non-empty group of Permissions and/or Prohibitions."@en ;
                                     <http://www.w3.org/2004/02/skos/core#note> "A Policy may contain multiple Rules."@en .


###  http://www.w3.org/ns/odrl/2/Rule
<http://www.w3.org/ns/odrl/2/Rule> rdf:type owl:Class .


###  https://w3id.org/def/vaeontology#ActingAgent
:ActingAgent rdf:type owl:Class ;
             rdfs:subClassOf :Agent ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn> ;
                               owl:someValuesFrom :Outcome
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :followsStrategy ;
                               owl:someValuesFrom :Strategy
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :makesAction ;
                               owl:allValuesFrom :Action
                             ] ;
             rdfs:comment "An Agent that makes Actions and/or follows some Strategy" ;
             rdfs:label "Acting Agent" .


###  https://w3id.org/def/vaeontology#Action
:Action rdf:type owl:Class ;
        rdfs:subClassOf :Outcome ;
        rdfs:comment "An action that produces some effect in the world, made possible by some ActingAgent." ;
        rdfs:label "Action" ;
        rdfs:seeAlso :ActingAgent .


###  https://w3id.org/def/vaeontology#ActionOnState
:ActionOnState rdf:type owl:Class ;
               rdfs:subClassOf :Outcome ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :onState ;
                                 owl:someValuesFrom :State
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :perform ;
                                 owl:someValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( :Action
                                                                    :Strategy
                                                                  )
                                                    ]
                               ] ;
               rdfs:comment "A policy that defines that, on some given State, an Action is to be made. These are the basic components of Strategies which followed by ActingAgents." ;
               rdfs:label "Action On State" .


###  https://w3id.org/def/vaeontology#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ,
                       <http://www.w3.org/ns/odrl/2/Party> ,
                       :VaeConditionedEntity ;
       rdfs:comment "An agent that takes part in a problem of interest, either by commiting to a ValueConcept or by participating in some Outcome." ;
       rdfs:label "Agent" .


###  https://w3id.org/def/vaeontology#AgentStatement
:AgentStatement rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description> ,
                                :VaeEntity ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isProposedByAgent ;
                                  owl:someValuesFrom :Agent
                                ] ;
                rdfs:comment "An AgentStatement is whatever statement proposed by an Agent in some context about the world." ;
                rdfs:label "Agent Statement" .


###  https://w3id.org/def/vaeontology#AggregationFunction
:AggregationFunction rdf:type owl:Class ;
                     rdfs:subClassOf :QuantitativeVaeProperty ;
                     rdfs:comment "An aggregation of some other VaeProperty over a set of Outcomes (or Sequence)." ;
                     rdfs:label "Aggregation Function" .


###  https://w3id.org/def/vaeontology#AlignmentSituation
:AlignmentSituation rdf:type owl:Class ;
                    rdfs:subClassOf :VaeSituation ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies> ;
                                      owl:someValuesFrom :AlignmentStatement
                                    ] ;
                    owl:disjointWith :AwarenessSituation ;
                    rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  \"When norms properly reflect values we say that the norms are value-aligned.” + (previously) “The application of norms always implies situation-awareness first.”" ,
                                 "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “… when the outcomes adequately abide by values - possibly because they satisfy entirely the (value-aligned) norms - we say that these outcomes are value-aligned.”" ;
                    rdfs:label "Alignment Statement" ;
                    <http://www.w3.org/2004/02/skos/core#note> "References for the specific ways of certifying alignment: [Nieves Montes, Carles Sierra et al. (Gini Index - Semantics Functions, Preferences,  Promotion, Demotion, Optimally aligned norms) https://www.jair.org/index.php/jair/article/view/13487], [Lera Leri et al. (“Moral Value Interpretation”) https://www.iiia.csic.es/media/filer_public/06/0a/060aec7a-2db6-40d7-b3e6-67de4260a065/2022aamaspluralisticvaluealignment.pdf], [Andrés Holgado-Sánchez (Admissibility of sequences of outcomes) https://link.springer.com/chapter/10.1007/978-3-031-43264-4_27]" .


###  https://w3id.org/def/vaeontology#AlignmentStatement
:AlignmentStatement rdf:type owl:Class ;
                    rdfs:subClassOf :VaeStatement ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :interpretationOn ;
                                      owl:someValuesFrom :ValueConcept
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :onEntity ;
                                      owl:allValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( :Norm
                                                                        :Outcome
                                                                      )
                                                        ]
                                    ] ;
                    owl:disjointWith :AwarenessStatement ;
                    rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  \"When norms properly reflect values we say that the norms are value-aligned.” + (previously) “The application of norms always implies situation-awareness first.”" ,
                                 "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “… when the outcomes adequately abide by values - possibly because they satisfy entirely the (value-aligned) norms - we say that these outcomes are value-aligned.”" ;
                    rdfs:label "Alignment Statement" ;
                    rdfs:seeAlso :AwarenessStatement ;
                    <http://www.w3.org/2004/02/skos/core#note> "References for the specific ways of certifying alignment: [Nieves Montes, Carles Sierra et al. (Gini Index - Semantics Functions, Preferences,  Promotion, Demotion, Optimally aligned norms) https://www.jair.org/index.php/jair/article/view/13487], [Lera Leri et al. (“Moral Value Interpretation”) https://www.iiia.csic.es/media/filer_public/06/0a/060aec7a-2db6-40d7-b3e6-67de4260a065/2022aamaspluralisticvaluealignment.pdf], [Andrés Holgado-Sánchez (Admissibility of sequences of outcomes) https://link.springer.com/chapter/10.1007/978-3-031-43264-4_27]" .


###  https://w3id.org/def/vaeontology#ArgumentStatement
:ArgumentStatement rdf:type owl:Class ;
                   rdfs:subClassOf :AgentStatement ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasClaim ;
                                     owl:someValuesFrom :VaeStatement
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isProposedByAgent ;
                                     owl:someValuesFrom :Agent
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPremise ;
                                     owl:allValuesFrom :VaeStatement
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isBasedOnCriterion ;
                                     owl:allValuesFrom :Criterion
                                   ] ;
                   rdfs:comment "An ArgumentStatement is a AgentStatment where a claim is made directly characterizing some value or norm related content (VaeStatement), justified by using other statements (premises) and based on some Criterion." ,
                                "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “One of the big advantages of explicit representations is that outcomes can be explained by the system itself in terms of norms and values. For example, a user can ask why he is blocked on a social media platform and this is then explained in terms of which community rules he broke and possibly what values these rules reflect (i.e. why the norm was adopted)”." ;
                   rdfs:label "Argument Statement" .


###  https://w3id.org/def/vaeontology#AwarenessSituation
:AwarenessSituation rdf:type owl:Class ;
                    rdfs:subClassOf :VaeSituation ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies> ;
                                      owl:someValuesFrom :AwarenessStatement
                                    ] ;
                    rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] “… a system can be given a formal representation of the norms and/or values that determine its behavior or the behavior it expects from its users. In this case we say that the system is norm and/or value-aware.”" ;
                    rdfs:label "Awareness Statement" .


###  https://w3id.org/def/vaeontology#AwarenessStatement
:AwarenessStatement rdf:type owl:Class ;
                    rdfs:subClassOf :VaeStatement ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :interpretationOn ;
                                      owl:someValuesFrom [ rdf:type owl:Class ;
                                                           owl:unionOf ( :Norm
                                                                         :ValueConcept
                                                                       )
                                                         ]
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :onEntity ;
                                      owl:someValuesFrom :Agent
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :onEntity ;
                                      owl:allValuesFrom :Agent
                                    ] ;
                    rdfs:comment "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] “… a system can be given a formal representation of the norms and/or values that determine its behavior or the behavior it expects from its users. In this case we say that the system is norm and/or value-aware.”" ;
                    rdfs:label "Awareness Statement" ;
                    rdfs:seeAlso :AlignmentStatement .


###  https://w3id.org/def/vaeontology#BooleanVaeProperty
:BooleanVaeProperty rdf:type owl:Class ;
                    rdfs:subClassOf :VaeProperty ;
                    rdfs:comment "A VaeProperty taken boolean values." ;
                    rdfs:label "Boolean Vae Property" .


###  https://w3id.org/def/vaeontology#ConceptNode
:ConceptNode rdf:type owl:Class ;
             rdfs:subClassOf :TaxonomyNode ;
             owl:disjointWith :PropertyNode ;
             rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
             rdfs:label "Concept Node" ;
             <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#Context
:Context rdf:type owl:Class ;
         rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
         rdfs:comment "A Context is any kind of entity that is underlying of specific events, situations, descriptions and agents. They can be used to describe, for example, the kind of problem or world where a system is deployed, contextualization of statements, etc." ;
         rdfs:label "Context" ;
         rdfs:seeAlso :VaeObject ;
         <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" ,
                                                            "https://arxiv.org/abs/2305.02748" .


###  https://w3id.org/def/vaeontology#Criterion
:Criterion rdf:type owl:Class ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Theory> ,
                           <http://www.w3.org/ns/odrl/2/Policy> ,
                           :AgentStatement ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/ns/odrl/2/action> ;
                             owl:someValuesFrom :VaeStatement
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/ns/odrl/2/assignee> ;
                             owl:someValuesFrom :Agent
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/ns/odrl/2/assigner> ;
                             owl:someValuesFrom :User
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :interpretationOn ;
                             owl:someValuesFrom :VaeConditioningEntity
                           ] ;
           rdfs:comment """In ArgumentStatements, the fact that specific claims can follow from some premises must be based on some Criterion (this class) proposed by an authority (i.e. the system designer or the researcher) or evolved by the society in some way. 

Criteria link premises and claims and provide constraints for the premises to be true, for the claim to follow from the premises, from the context, etc.""" ;
           rdfs:label "Criterion" ;
           <http://www.w3.org/2004/02/skos/core#note> """[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]  “One of the big advantages of explicit representations is that outcomes can be explained by the system itself in terms of norms and values. For example, a user can ask why he is blocked on a social media platform and this is then explained in terms of which community rules he broke and possibly what values these rules reflect (i.e. why the norm was adopted)”. 

Criteria are those conditions that identify value-aligned or missaligned behaviour with values or norms in mind.""" .


###  https://w3id.org/def/vaeontology#Equality
:Equality rdf:type owl:Class ;
          rdfs:subClassOf <https://w3id.org/spice/SON/SchwartzValues#SelfTrascendence> ;
          rdfs:comment "Equality is present in most of the VAE papers so far, as it is easy to formalize mathematically." ;
          rdfs:label "Equality" .


###  https://w3id.org/def/vaeontology#FinalInputOutcome
:FinalInputOutcome rdf:type owl:Class ;
                   rdfs:subClassOf :MultiInputOutcome ;
                   rdfs:comment "The final relation between inputs and outcomes of a Sequence." ;
                   rdfs:label "Final Input Outcome" ;
                   rdfs:seeAlso :Sequence ,
                                "http://ontologydesignpatterns.org/wiki/Submissions:Sequence" .


###  https://w3id.org/def/vaeontology#Generalization
:Generalization rdf:type owl:Class ;
                rdfs:subClassOf :ValueComparisonStatement ;
                owl:disjointWith :ImportanceRelation ;
                rdfs:comment "A comparison statement proposed by an Agent in which believes that some ValueConcept (superior) generalizes another one (inferior)." ;
                rdfs:label "Generalization" ;
                rdfs:seeAlso :generalizesNode .


###  https://w3id.org/def/vaeontology#GiniIndex
:GiniIndex rdf:type owl:Class ;
           rdfs:subClassOf :SemanticsFunction ;
           rdfs:comment "The Gini Index can be used as a Semantics Function in State, as in the taxing problem from Montes and Sierra (see reference)." ;
           rdfs:label "Gini Index" ;
           <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Montes, N., Sierra, C.: Synthesis and properties of optimally value-aligned nor-
mative systems. Journal of Artificial Intelligence Research 74, 1739–1774 (2022).
https://doi.org/10.1613/jair.1.13487""" .


###  https://w3id.org/def/vaeontology#ImportanceRelation
:ImportanceRelation rdf:type owl:Class ;
                    rdfs:subClassOf :ValueComparisonStatement ;
                    rdfs:comment "\"Another important concept considered core in the social sciences is value priority, or how important a value is for someone.\"  [Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ,
                                 "A comparison statement proposed by an Agent in which believes that some ValueConcept (superior) is more important than another one (inferior)." ;
                    rdfs:label "Importance Relation" ;
                    rdfs:seeAlso "http://hdl.handle.net/10261/197622" ;
                    <https://w3id.org/spice/SON/SchwartzValues#bibRef> "Value Systems as a ranking between values: [Serramia et al. https://aamas.csc.liv.ac.uk/Proceedings/aamas2018/pdfs/p1294.pdf]" .


###  https://w3id.org/def/vaeontology#MultiInputOutcome
:MultiInputOutcome rdf:type owl:Class ;
                   rdfs:subClassOf :Outcome ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isStartOfSequence ;
                                     owl:allValuesFrom :Sequence
                                   ] ;
                   rdfs:comment "A relationship between a series of input dul:Events and a series of Outcomes that follow those. MultiInputOutcomes are the basic blocks to build Sequences, they follow the Sequence ODP." ;
                   rdfs:label "Multi Input Outcome" ;
                   rdfs:seeAlso :Sequence ,
                                "http://ontologydesignpatterns.org/wiki/Submissions:Sequence" .


###  https://w3id.org/def/vaeontology#NodeImportance
:NodeImportance rdf:type owl:Class ;
                rdfs:subClassOf :QuantitativeVaeProperty ,
                                :ValueProperty ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :propertyExpressedAt ;
                                  owl:someValuesFrom :TaxonomyNode
                                ] ;
                rdfs:comment "\"Another important concept considered core in the social sciences is value priority, or how important a value is for someone. We incorporate this concept by attaching a mea- sure of importance to each node of the taxonomy.\"  [Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                rdfs:label "Node Importance" ;
                rdfs:seeAlso :ImportanceRelation ,
                             :ValueTaxonomy .


###  https://w3id.org/def/vaeontology#Norm
:Norm rdf:type owl:Class ;
      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Norm> ,
                      <http://www.w3.org/ns/odrl/2/Policy> ,
                      :VaeConditionedEntity ,
                      :VaeConditioningEntity ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/odrl/2/action> ;
                        owl:someValuesFrom :Outcome
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/odrl/2/assignee> ;
                        owl:someValuesFrom :Agent
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/odrl/2/assigner> ;
                        owl:someValuesFrom :User
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/odrl/2/target> ;
                        owl:someValuesFrom :System
                      ] ;
      owl:disjointWith :ValueTaxonomyStatement ;
      rdfs:comment "\"Norms (...) establish boundaries, either soft or hard, on individual autonomy through a variety of mechanisms such as social pressure and expectations, constraints on actions, and sanctions for violation or rewards for compliance.\"" ,
                   "[Luc Steels: https://vale2023.iiia.csic.es/accepted-papers] \"Explicit representations of norms are often called policies.\"" ;
      rdfs:label "Norm" ;
      <https://w3id.org/spice/SON/SchwartzValues#bibRef> "Nieves Montes, Nardine Osman, and Carles Sierra, ‘Perspective-dependent value alignment of norms’ VALE workshop, ECAI 2023" .


###  https://w3id.org/def/vaeontology#OptimizedProperty
:OptimizedProperty rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :isOptimizationOfProperty ;
                                         owl:someValuesFrom :VaeProperty
                                       ] ;
                   rdfs:subClassOf :BooleanVaeProperty ,
                                   :QuantitativeVaeProperty ;
                   rdfs:comment "Assertion that a property has being optimized over a set of possible outcomes, supplying the value achieved (e.g. an optimal (maximum) vae:GiniIndex value of 0.9 is vae:measuredOnOutcome vae:State, over some other possible states -the optimization domain could be represented somewhere else or with a custom property-)." ;
                   rdfs:label "Optimized Property" .


###  https://w3id.org/def/vaeontology#Outcome
:Outcome rdf:type owl:Class ;
         rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ,
                         :VaeConditionedEntity ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :composedBySequence ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Sequence
                         ] ;
         rdfs:comment "Outcomes include the effect of the behavior of a system or of a user using a system [Luc Steels: https://vale2023.iiia.csic.es/accepted-papers]." ;
         rdfs:label "Outcome" .


###  https://w3id.org/def/vaeontology#Path
:Path rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Sequence
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasInputOutcome ;
                                                   owl:allValuesFrom :Transition
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :Sequence .


###  https://w3id.org/def/vaeontology#PromotionDemotion
:PromotionDemotion rdf:type owl:Class ;
                   rdfs:subClassOf :QualitativeVaeProperty ,
                                   :ValueProperty ;
                   rdfs:comment "A QualitativeVaeProperty and a ValueProperty that can represent Promotion, Demotion or Neutrality of an Outcome towards some Value." ;
                   rdfs:label "Promotion Demotion" .


###  https://w3id.org/def/vaeontology#PropertyNode
:PropertyNode rdf:type owl:Class ;
              rdfs:subClassOf :TaxonomyNode ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasPropertyTaxonomy ;
                                owl:someValuesFrom :TaxonomyProperty
                              ] ;
              rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
              rdfs:label "Property Node" ;
              <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#QualitativeVaeProperty
:QualitativeVaeProperty rdf:type owl:Class ;
                        rdfs:subClassOf :VaeProperty ;
                        rdfs:comment "A VaeProperty that represent a qualitative consideration on an Outcome with respect to a Value or Norm." ;
                        rdfs:label "Qualitative Vae Property" .


###  https://w3id.org/def/vaeontology#QuantitativePromotionDemotion
:QuantitativePromotionDemotion rdf:type owl:Class ;
                               rdfs:subClassOf :PromotionDemotion ,
                                               :QuantitativeVaeProperty ;
                               rdfs:comment "A quantitative version of PromotionDemotion that specifies some degre of Promotion/Demotion/Neutrality." ;
                               rdfs:label "Quantitative Promotion Demotion" .


###  https://w3id.org/def/vaeontology#QuantitativeVaeProperty
:QuantitativeVaeProperty rdf:type owl:Class ;
                         rdfs:subClassOf :VaeProperty ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty rdf:value ;
                                           owl:someValuesFrom owl:real
                                         ] ;
                         rdfs:comment "A VaeProperty that is represented by some numeric value." ;
                         rdfs:label "Quantitative Vae Property" .


###  https://w3id.org/def/vaeontology#SemanticsFunction
:SemanticsFunction rdf:type owl:Class ;
                   rdfs:subClassOf :QuantitativeVaeProperty ,
                                   :ValueProperty ;
                   rdfs:comment "\"To express the relationship between an abstract value and the meaning it takes in a particular context, we say that an explicit goal g grounds the semantics of value v in context C\" [Nieves Montes, Carles Sierra et al. https://www.jair.org/index.php/jair/article/view/13487]" ;
                   rdfs:label "Semantics Function" ;
                   <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Montes, N., Sierra, C.: Synthesis and properties of optimally value-aligned nor-
mative systems. Journal of Artificial Intelligence Research 74, 1739–1774 (2022).
https://doi.org/10.1613/jair.1.13487""" .


###  https://w3id.org/def/vaeontology#Sequence
:Sequence rdf:type owl:Class ;
          rdfs:subClassOf :Outcome ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :startsWithInputOutcome ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :MultiInputOutcome
                          ] ;
          rdfs:comment "A Sequence of Outcomes. In general, allows representing sequences of input dul:Events and their related Outcomes (e.g. series of states -> actions, newstates -> actions, etc). It follows the Sequence ODP." ;
          rdfs:label "Sequence" ;
          rdfs:seeAlso "http://ontologydesignpatterns.org/wiki/Submissions:Sequence" .


###  https://w3id.org/def/vaeontology#SoundArgument
:SoundArgument rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/odrl/2/Operator> ;
               rdfs:comment "Any operator used in a Constraint for a Criterion, between premises and claims contrasting if the argument is sound." ;
               rdfs:label "Sound Argument" .


###  https://w3id.org/def/vaeontology#State
:State rdf:type owl:Class ;
       rdfs:subClassOf :Outcome ;
       rdfs:comment "A representation of the State of an Agent." ;
       rdfs:label "State" .


###  https://w3id.org/def/vaeontology#Strategy
:Strategy rdf:type owl:Class ;
          rdfs:subClassOf :Outcome ;
          rdfs:comment "A mapping between States and Actions that an ActingAgent follows and regulate its behaviour in the world." ;
          rdfs:label "Strategy" .


###  https://w3id.org/def/vaeontology#System
:System rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Collection> ,
                        :Agent ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasTrace ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Sequence
                        ] ;
        owl:disjointWith :User ;
        rdfs:comment "A system is the organization of a set of Agents modelling some complex joint behaviour or for solving a task. A system has a trace, a Sequence of the inputs and the corresponding Outcomes performed (e.g.) by the system or the agents on it." ;
        rdfs:label "System" .


###  https://w3id.org/def/vaeontology#TaxonomyAlignment
:TaxonomyAlignment rdf:type owl:Class ;
                   rdfs:subClassOf :QuantitativeVaeProperty ,
                                   :ValueProperty ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :propertyExpressedAt ;
                                     owl:someValuesFrom :ValueTaxonomyStatement
                                   ] ;
                   rdfs:comment "\"We argue that the property-based nodes of a value taxonomy introduce the foundations for linking abstract value concepts to concrete computational constructs that can help formally assess the alignment of behaviour with those values. The value alignment of an entity’s behaviour becomes the degree of satisfaction of the property-based value nodes of the relevant taxonomy that this behaviour brings about.\"" ;
                   rdfs:label "Taxonomy Alignment" ;
                   rdfs:seeAlso :ValueTaxonomy ;
                   <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#TaxonomyNode
:TaxonomyNode rdf:type owl:Class ;
              rdfs:subClassOf :VaePropertyStatement ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isTaxonomyNodeOf ;
                                owl:someValuesFrom :ValueTaxonomyStatement
                              ] ;
              rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
              rdfs:label "A node of a ValueTaxonomy." ;
              rdfs:seeAlso :ValueTaxonomyStatement ;
              <https://w3id.org/spice/SON/SchwartzValues#bibRef> """Osman, N., d’Inverno, M.: A computational framework of human values for ethical
ai (2023)""" .


###  https://w3id.org/def/vaeontology#TaxonomyProperty
:TaxonomyProperty rdf:type owl:Class ;
