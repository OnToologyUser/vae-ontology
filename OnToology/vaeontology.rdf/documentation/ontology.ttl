                                    owl:onProperty :propertyExpressedAt ;
                                    owl:someValuesFrom :PropertyNode
                                  ] ;
                  rdfs:comment "[Nardine Osman, Mark D’Inverno https://arxiv.org/abs/2305.02748]" ;
                  rdfs:label "Property Taxonomy" .


###  https://w3id.org/def/vaeontology#TextualVaeProperty
:TextualVaeProperty rdf:type owl:Class ;
                    rdfs:subClassOf :VaeProperty ;
                    rdfs:comment "A VaeProperty expressed in natural language." ;
                    rdfs:label "Textual Vae Property" .


###  https://w3id.org/def/vaeontology#Transition
:Transition rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :MultiInputOutcome
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasInput ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass :Action
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasInput ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass :State
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasOutcome ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass :State
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf :Outcome .


###  https://w3id.org/def/vaeontology#User
:User rdf:type owl:Class ;
      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Person> ,
                      :Agent ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isUserOfSystem ;
                        owl:someValuesFrom :System
                      ] ;
      rdfs:comment "A User is a human agent that can take part in a system (or model it, supervise it, interact with it...)" ;
      rdfs:label "User" .


###  https://w3id.org/def/vaeontology#VaeConditionedEntity
:VaeConditionedEntity rdf:type owl:Class ;
                      rdfs:subClassOf :VaeStatementEntity .


